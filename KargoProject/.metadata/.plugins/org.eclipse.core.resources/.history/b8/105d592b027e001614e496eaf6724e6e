package com.Kargo.helper;

public class XpathHelper {
	
	/*
	 * xpath for Google Search text filed
	 */
	public static String googleSearchTextField="//input[@title='Search']";
	
	/*
	 * xpath for Google Search Submit button
	 */
	public static String googleSearchSubmitButton="//button[@type='submit']";
	
	/*
	 * xpath for Kargo Website
	 */
	public static String kargoHomePage="//a[@href='http://www.kargo./']";
	
	/*
	 * xpath for Kargo Main Menu Button
	 */
	public static String kargoMainMenuButton="//a[@class='nav-button js-nav-toggle']";
	
	/*
	 * xpath for Kargo About-us Option
	 */
	public static String kargoAboutUsOption="//a[@href='http://www.kargo.com/about/']";
	
	
	
	
	
	

}
